def pars(x):
  #Данная функция берет на вход файл протеома бактерии и вносит информацию
  #из него в двумерный список, где первый индекс нумерует порядок белков в протеоме,
  #а второй - порядок строк внутри каждого белка.
    with open(x, mode='r') as f:
        S=[]
        s=[]
        for i in f:
          if i[0]+i[1] != '//':
            i.strip()
            i = i[0:len(i)-1]
            s.append(i)
          else: 
                S.append(s)
                s=[]
    return S
